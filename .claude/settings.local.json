{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(./github-actions-improver.py --mode create)",
      "Bash(chmod:*)",
      "Bash(ls:*)",
      "Bash(python3:*)",
      "Bash(PYTHONPATH=. python3 enhanced-concurrent-fixer.py --days 7)",
      "Bash(./install-enhanced.sh:*)",
      "Bash(git add:*)",
      "Bash(gh run list:*)",
      "Bash(gh workflow:*)",
      "Bash(gh run view:*)",
      "Bash(git push:*)",
      "Bash(python -m pytest tests/test_enhanced_concurrent_fixer.py -v)",
      "Bash(python -m pytest tests/ --cov=. --cov-report=term-missing --tb=short)",
      "Bash(python -m pytest tests/test_failure_analyzer.py -v)",
      "Bash(git commit:*)",
      "Bash(python -m pytest tests/test_claude_token_setup.py -v)",
      "Bash(timeout:*)",
      "Bash(python -m pytest tests/test_gha_jobs_commands.py -v --tb=short)"
    ],
    "deny": []
  }
}